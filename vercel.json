{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "buildCommand": "cd frontend-dashboard-deploy && pnpm run build",
  "installCommand": "cd frontend-dashboard-deploy && pnpm install",
  "outputDirectory": "frontend-dashboard-deploy/dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
